@model List<HeroWrapDto>
@{
    ViewData["Title"] = "Default";
}

		@foreach (var item in Model)
		{
			<section class="hero-wrap js-fullheight" style="background-image: url(Assets/images/HeroWrap/@item?.Image);" data-stellar-background-ratio="0.5">
						<div class="overlay"></div>
								<div class="container">
									<div class="row no-gutters slider-text js-fullheight justify-content-center align-items-center">
										<div class="col-lg-12 ftco-animate d-flex align-items-center">
											<div class="text text-center">
									<span class="subheading"> @item?.Title</span>
									<h1 class="mb-4">@item?.Project</h1>
												<p><a href="#" class="btn btn-primary btn-outline-primary px-4 py-2">Book now</a></p>
											</div>
										</div>
									</div>
								</div>
			</section>
			
		}		

	





