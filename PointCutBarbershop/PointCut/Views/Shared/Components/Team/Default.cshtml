@model List<TeamDto>
@{
    ViewData["Title"] = "Default";
}

<section class="ftco-section ftco-team">
	<div class="container-fluid px-md-5">
		<div class="row justify-content-center pb-3">
			<div class="col-md-10 heading-section text-center ftco-animate">
				<span class="subheading">Artistic Director</span>
				<h2 class="mb-4">Makeup Artist</h2>
				<p>Far far away, behind the word mountains, far from the countries Vokalia and Consonantia</p>
			</div>
		</div>
		<div class="row">
			<div class="col-md-12 ftco-animate">
				<div class="carousel-team owl-carousel">
					@foreach (var item in Model)
					{
						<div class="item">
						<a href="#" class="team text-center">
							<div class="img" style="background-image: url(Assets/images/Team/@item?.Image);"></div>
							<h2>@item?.Name</h2>
							<span class="position">@item?.Job</span>
						</a>
					</div>
					}
